{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es6",
    "outDir": "out",
    "lib": ["es6", "dom"],
    "sourceMap": true,
    "rootDir": ".",
    "strict": true,
    "noUnusedLocals": false,
    "noUnusedParameters": true,
    "allowUnusedLabels": false,
    "strictNullChecks": true
  },
  "exclude": ["node_modules", ".vscode-test"]
}
